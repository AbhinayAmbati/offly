{
  "name": "offly",
  "version": "0.1.1",
  "description": "A lightweight plugin that makes any web app offline-first automatically",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "offly": "./dist/cli/index.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepublishOnly": "npm run build && npm run test"
  },
  "keywords": [
    "offline-first",
    "service-worker",
    "cache",
    "pwa",
    "background-sync",
    "typescript",
    "cli"
  ],
  "author": "Abhinay Ambati",
  "mail": "abhinayambati4@gmail.com",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/AbhinayAmbati/offly.git"
  },
  "bugs": {
    "url": "https://github.com/AbhinayAmbati/offly/issues"
  },
  "sponsor": {
    "url": "https://github.com/sponsors/AbhinayAmbati"
  },
  "homepage": "https://github.com/AbhinayAmbati/offly#readme",
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "dexie": "^3.2.4",
    "fs-extra": "^11.1.1",
    "glob": "^10.3.10",
    "workbox-build": "^7.0.0",
    "workbox-sw": "^7.0.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.8.10",
    "@types/react": "^18.3.26",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.1.1",
    "@vitest/coverage-v8": "^0.34.6",
    "@vitest/ui": "^0.34.6",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jsdom": "^22.1.0",
    "prettier": "^3.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vite-plugin-dts": "^3.6.3",
    "vitest": "^0.34.6"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "vue": ">=3.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "vue": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
